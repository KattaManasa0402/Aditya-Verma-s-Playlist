// Online C++ compiler to run C++ program online
//PERMUTATION WITH SPACES
#include <iostream>
#include<string>
using namespace std;
void solve(string inp,string op){
    //base
    if(inp.size()==0){
        cout<<op<<endl;
        return;
    }
    string op1=op;
    string op2=op;
    op1.push_back('_');
    op1.push_back(inp[0]);
    op2.push_back(inp[0]);
    inp.erase(inp.begin()+0);
    solve(inp,op1);
    solve(inp,op2);
    return;
}
int main() {
    string inp="ABC";
    string op="";
    op.push_back(inp[0]);
    inp.erase(inp.begin()+0);
    solve(inp,op);
    return 0;
}
